});

function embedModalOpen() {
  var embedModal = document.querySelector('#embedModal');
  if (embedModal) {
    mixpanel.track('Embed collection');
    embedModal.classList.add('Sharing-modal--visible');
    var snippet = document.querySelector('#Sharing-snippet');
    snippet.focus();
    snippet.select();
  }
}

function embedModalClose() {
  var embedModal = document.querySelector('#embedModal');
  if (embedModal) {
    embedModal.classList.remove('Sharing-modal--visible');
  }
}</script><script>window.installLink = 'https://chrome.google.com/webstore/detail/hddnkoipeenegfoeaoibdmnaalmgkpip';
